{
  "routes": [
    {
      "route": "/",
      "serve": "/home",
      "statusCode": 200
    },
    {
      "route": "/viewer",
      "allowedRoles": ["contributor"]
    },
    {
      "route": "api/deepzoom/*",
      "allowedRoles": ["contributor"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    }
  },
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
