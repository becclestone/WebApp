{
  "routes": [
    {
      "route": "/",
      "serve": "/home",
      "statusCode": 200
    },
    {
      "route": "/doctor",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/viewer",
      "allowedRoles": ["contributor"]
    },
    {
      "route": "/viewer2",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/deepzoom/*",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/profile",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/annotation",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/questions",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/progress",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/api/answers",
      "allowedRoles": ["contributor","reader"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    }
  },
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
